{
  "expo": {
    "name": "RentalRoom",
    "slug": "RentalRoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RentalRoom.png",
    "scheme": "rentalroom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/images/RentalRoom.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/RentalRoom.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "extra": {
      "API_URL_BACKEND": "https://faultier-nonaristocratically-willene.ngrok-free.dev"
    }
  }
}
